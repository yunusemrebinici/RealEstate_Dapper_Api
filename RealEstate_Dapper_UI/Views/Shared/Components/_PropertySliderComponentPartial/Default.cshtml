@model List<GetProductImageByProductIdDto>

<div class="single-post-image mb-5">
	<div class="owl-blog owl-carousel owl-theme">
		@foreach (var item in Model)
		{
			<div class="item">
				<div class="card">
					<img src="@item.ImageUrl" class="img-fluid radius-image" style="height:500px; width:850px;" height="600px">
				</div>
			</div>
		}
	</div>
</div>